n = int(input())
if n % 100 == 11:
    print (n, 'программистов')
elif n % 100 == 12:
    print(n, 'программистов')
elif n % 100 == 13:
    print(n, 'программистов')
elif n % 100 == 14:
    print(n, 'программистов')
elif n % 10 == 2:
    print(n, 'программиста')
elif n % 10 == 3:
    print (n, 'программиста')
elif n % 10 == 4:
    print (n, 'программиста')
elif n % 10 == 1:
    print(n, 'программист')
else:
    print(n, 'программистов')
